{
  "manifest_version": 3,
  "name": "Phishing Detector (NVD-based)",
  "version": "1.0",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://services.nvd.nist.gov/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Phishing Detector",
    "default_popup": "popup.html",
  "default_icon": "icon.png"
  }
}
